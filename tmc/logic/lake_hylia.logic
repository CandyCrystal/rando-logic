area LakeHylia:
  room BeanstalkArea:
    node EastArea:
      door LeftLadder
      door RightLadder
    node WestArea:
      door Ladder
    node BeanstalkFusion:
      entrance BeanStalkFusion
    node BeanStalkFusionExit:
      exit BeanStalkFusion
    BeanStalkFusionExit -> WestArea: flag HyliaBeanstalkFusion
    WestArea -> BeanstalkFusion: flag HyliaBeanstalkFusion
    node EastArea +: entrance Virtual.Zeffa.FluteAway
    node WestArea +: entrance Virtual.Zeffa.FluteAway
  room Main:
    node Crest:
      exit Crest
      set Virtual.Zeffa.LakeCrest
    node CrestMinish:
      door CrestMinishHouse
      exit CrestHoleFusion
    Crest -> CrestMinish: item CanTurnMinish
    CrestMinish -> Crest: item CanGrow
    node CrestHoleFusion:
      entrance CrestHoleFusion
    CrestMinish -> CrestHoleFusion: flag CrestHoleFusion
    node MinishWater:
      exit MinishSouth
      exit NorthHole
    node MinishWaterEntrances:
      entrance South
      entrance NorthHole
    MinishWater -> MinishWaterEntrances: item Flippers
    node EastVineHole:
      door EastVineHole
    MinishWater <-> EastVineHole: item Flippers
    node Water:
      chest SouthHP
      enemy WaterEnemies
    node DigCaveFusionEntrance:
      entrance DigCaveFusion
    node DigCaveFusionExit:
      exit DigCaveFusion
    node DigCaveFusionIsland
    DigCaveFusionExit -> DigCaveFusionIsland: flag HyliaDigCaveFusion
    DigCaveFusionIsland -> DigCaveFusionEntrance: item MoleMitts
    Water -> DigCaveFusionIsland: (flag HyliaDigCaveFusion & (item Flippers | item Cape))
    DigCaveFusionExit -> Water: item Flippers
    node IslandTopMinish:
      door IslandMinishHouse
      door Hole
    IslandTop -> MinishWater: (item Flippers & setting LakeMinishWaterJump)
    node SouthWest:
      door East
      door EastSouth
      door South
      enemy SouthWestChus
    node DropletsEntrance:
      door DropletsEntrance
    Water <-> DropletsEntrance: (item Cape | item Flippers)
    node FifiWaterHole:
      chest FifiWaterHoleHP
    node West:
      set Boulder
      door Tree
      enemy WestEnemies
    node NorthWest:
      door Fifi
      door West
      enemy ChuAndOctorok
    NorthWest <-> FifiWaterHole: item Flippers
    West <-> Water: (item Cape | item Flippers)
    West <-> NorthWest: flag Boulder
    Crest -> NorthWest
    node SouthEast:
      door Ladder
      door Cabin
    SouthEast <-> Water: (item Cape | item Flippers)
    node MinishPortalBonk:
      set TreeBonk
    node MinishPortal:
      door MinishPath
    SouthEast -> MinishPortalBonk: item Boots
    SouthEast -> MinishPortal: (item CanTurnMinish & flag TreeBonk)
    MinishPortal <-> MinishWater: item Flippers
    MinishPortal -> SouthEast: item CanGrow
    node DigCaveNorthArea:
      exit DigCave
    node NorthHP:
      chest NorthHP
    DigCaveNorthArea <-> NorthHP: item Cape
    node DigCaveNorthEntrance:
      entrance DigCave
    DigCaveNorthArea -> DigCaveNorthEntrance: item MoleMitts
    DigCaveNorthArea <-> NorthWest: item Cape
    DigCaveNorthArea -> Water: item Flippers
    NorthWest <-> Water: (item Cape | item Flippers)
    node NorthWest +: entrance Virtual.Zeffa.FluteAway
    node DigCaveNorthArea +: entrance Virtual.Zeffa.FluteAway
    node SouthEast +: entrance Virtual.Zeffa.FluteAway
    node SouthWest +: entrance Virtual.Zeffa.FluteAway
    node DigCaveFusionIsland +: entrance Virtual.Zeffa.FluteAway
